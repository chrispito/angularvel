{
  "name": "angularvel",
  "version": "0.0.1",
  "description": "",
  "main": "",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "lint": "tslint ./resources/assets/js/angular/src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "start:fr": "ng serve --aot --i18nFile=resources/assets/js/angular/src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "build": "tsc -p resources/assets/js/angular/src/",
    "build:fr": "ng build --prod --i18nFile=resources/assets/js/angular/src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "extract": "ng xi18n --outputPath=resources/assets/js/angular/src/locale",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test:once": "karma start karma.conf.js --single-run",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "pretest:once": "npm run build",
    "build:watch": "tsc -p resources/assets/js/angular/src/ -w",
    "build:upgrade": "tsc",
    "serve:upgrade": "http-server",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "build:babel": "babel resources/assets/js/angular/src -d resources/assets/js/angular/src --extensions \".es6\" --source-maps",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n"
  },
  "author": "Chrispito",
  "license": "MIT",
